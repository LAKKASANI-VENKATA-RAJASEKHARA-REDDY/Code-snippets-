#include<stdio.h>
main()
{
	int arr[5],i;
	int max,min;
	for(i=0;i<=4;i++)
	{
		printf("ENTER VALUE: ");
		scanf("%d",&arr[i]);
	}
		printf("array is ");
	for(i=0;i<=4;i++)
	{
		printf("%d ",arr[i]);
	}
	printf("\n");
	max=arr[0];min=arr[4];
	for(i=0;i<5;i++)
	{
		if(arr[0]<arr[i])
		{
			max=arr[i];
		}
		if(arr[4]>arr[i])
		{
			min=arr[i];
		}
	}
	printf("maximum value is %d ",max);
	printf("minimum value is %d ",min);
}
