#include <stdio.h>

struct students {
    int rollnumber;
    char name[20];
    float percentage;
    char grade;
};

int main()
{
    struct students s1 = {1, "RAAKUMARI", 99.9, 'A'};

    printf("INFORMATION ABOUT S1:\n");
    printf("Roll Number: %d\n", s1.rollnumber);
    printf("Name: %s\n", s1.name);
    printf("Percentage: %.2f\n", s1.percentage);
    printf("Grade: %c\n", s1.grade);

    return 0;
}
